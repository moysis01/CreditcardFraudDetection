{
  "classifiers": ["XGBoost", "KNN"],
  "tuned_parameters": {
    "XGBoost": {
      "learning_rate": [0.01, 0.1, 0.2],
      "n_estimators": [100, 200, 300]
    },
    "KNN": {
      "n_neighbors": [3, 5, 7],
      "weights": ["uniform", "distance"]
    }
  },
  "hyperparameter_tuning": true,
  "cross_validation": true,
  "ensemble": true,
  "voting": "soft",
  "resampling": {
    "method": "SMOTE",
    "params": {
      "random_state": 25,
      "sampling_strategy": "auto",
      "k_neighbors": 5
    }
  },
  "scaling": "MinMaxScaler"
}
